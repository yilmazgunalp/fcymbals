
<h1>  <%=params[:merchant] %>  </h1>
<% @rsp.each do |r,makers| %>

<h3><%=r.title %></h4>
<p><%=r.id %></p>
<a href= <%=r.link %> target = 'blank'>Product Link</a>
<div class="makers">
<% makers.each do |m|  %>
	<div class="maker">
		<ul>
			<% m.each do |k,v|  %>
				<li> <span class="key"><%= k %></span>	: <span class="val"> <%= v %></span>	</li>
			<% end %>
		</ul>

		<button>Allocate</button>

	</div>
<% end %>

</div>

<% end %>


