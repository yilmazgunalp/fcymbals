<% content_for :css do %>
 <%= stylesheet_link_tag 'svg','form','results', media: 'all'%>
 <%= javascript_include_tag 'searches' %>
 <meta name="Mediapartners-Google" content="index">
 <meta name="robots" content="noindex, nofollow">
<% end %>

<div id="wrapper">
	<%=render 'shared/left' %>
	<div id="container">
		<%=render 'shared/logo' %>
		<%= render 'shared/form' %>
		
		<!-- SEARCH RESULTS -->
		<main id="results">
			<div class="facets-container">

			</div>
			<div class="results-container">
				<% if @no_result%>
				<%= render partial: 'shared/noresult' %>	
				<% end %>
				<%= render partial: 'shared/result',  collection: @retailers %>

				
			</div><!-- Results Container -->
		</main>
		<!-- END SEARCH RESULTS -->
		</div>
	<%= render 'shared/right' %>
	
</div><!-- div#wrapper -->
<%= render 'shared/footer' %>
